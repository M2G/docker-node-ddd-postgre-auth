{
  "name": "docker-nodejs-typescript",
  "version": "0.0.1",
  "description": "Docker, NodeJS and Typescript",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/M2G/docker-nodejs-typescript-starter-pack"
  },
  "homepage": "https://github.com/M2G/docker-nodejs-typescript-starter-pack",
  "bugs": {
    "url": "https://github.com/M2G/docker-nodejs-typescript-starter-pack/issues"
  },
  "keywords": [
    "docker",
    "nodejs",
    "typescript"
  ],
  "author": "M2G",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node --harmony-top-level-await build/src/index.js",
    "watch-node": "nodemon build/src/index.js",
    "watch": "NODE_ENV=development concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect build/src/index.js",
    "test": "jest unit --forceExit --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.10",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.0.27",
    "@types/ramda": "^0.27.40",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "assert": "^1.4.1",
    "await-to-ts": "^1.0.6",
    "awilix": "^4.3.3",
    "bcrypt": "^5.0.1",
    "body-parser": "1.18.3",
    "concurrently": "^6.0.2",
    "cors": "2.8.5",
    "eslint": "7.25.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "4.16.4",
    "express-graphql": "^0.12.0",
    "http-status": "^1.5.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul": "^0.4.5",
    "jest": "27.0.4",
    "jest-cli": "^27.0.4",
    "jest-resolve": "27.0.4",
    "jest-watch-typeahead": "0.6.4",
    "jsonwebtoken": "8.5.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.3",
    "prettier": "^2.1.2",
    "ramda": "0.26.1",
    "redis": "^3.1.2",
    "sequelize": "^6.6.2",
    "structure": "^2.0.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.2.4",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.11.2",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/pg": "^7.14.11",
    "babel-eslint": "^10.0.3",
    "concurrently": "^6.0.2",
    "supertest": "3.4.2",
    "supertest-as-promised": "^4.0.2",
    "ts-node": "^10.0.0"
  }
}
